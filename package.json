{
  "name": "electronapp",
  "version": "1.0.0",
  "description": "A Playground for Electron",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "build": "electron-builder"
  },
  "build": {
    "appId": "com.example.electronapp",
    "productName": "ElectronApp",
    "directories": {
      "output": "build"
    },
    "files": [
      "main.js",
      "index.html",
      "styles.css",
      "renderer.js",
      "preload.js"
    ],
    "win": {
      "target": "portable"
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "AppImage"
    },
    "publish": null
    
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harlock123/ElectronApp.git"
  },
  "author": "Lonnie Watson",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/harlock123/ElectronApp/issues"
  },
  "homepage": "https://github.com/harlock123/ElectronApp#readme",
  "devDependencies": {
    "electron": "^23.2.0",
    "electron-builder": "^23.6.0"
  }
}
